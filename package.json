    "@radix-ui/react-slot": "^1.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "apollo-server-express": "^3.13.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "lucide-react": "^0.424.0",
    "mongoose": "^8.5.2",
    "next": "14.2.5",
    "nodemon": "^3.1.4",
    "query-string": "^9.1.0",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.14.14",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "concurrently": "^6.2.0"
  },
