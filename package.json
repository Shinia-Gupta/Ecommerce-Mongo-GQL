{
  "name": "ecom-gql-copy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "proxy": "http://localhost:5000/graphql",
  "type": "module",

  "scripts": {
  "dev": "concurrently \"npm run dev-client\" \"npm run dev-server\"",
  "build": "npm run build-client",
  "start": "npm run start-client && npm run start-server",
  "dev-client": "cd client && next dev --port 3000",
  "build-client": "cd client && next build",
  "start-client": "cd client && next start --port 3000",
  "lint-client": "cd client && next lint",
  "dev-server": "cd server && nodemon index.js",
  "start-server": "cd server && node index.js"
},

  "dependencies": {
    "@apollo/client": "^3.11.2",
    "@apollo/experimental-nextjs-app-support": "^0.11.2",
